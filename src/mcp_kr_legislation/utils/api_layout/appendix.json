    {
      "id": "1",
      "title": "법령 별표ㆍ서식 목록 조회 API",
      "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=licbyl",
      "target": "licbyl",
      "api_type": "목록조회",
      "sub_category": "법령",
      "parameters": [
        {
          "name": "OC",
          "type": "string(필수)",
          "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
        },
        {
          "name": "target",
          "type": "string : licbyl(필수)",
          "description": "서비스 대상"
        },
        {
          "name": "type",
          "type": "char(필수)",
          "description": "출력 형태 HTML/XML/JSON"
        },
        {
          "name": "search",
          "type": "int",
          "description": "검색범위 (기본 : 1 별표서식명) 2 : 해당법령검색 3 : 별표본문검색"
        },
        {
          "name": "query",
          "type": "string",
          "description": "검색을 원하는 질의(default=*) (정확한 검색을 위한 문자열 검색 query=\"자동차\")"
        },
        {
          "name": "display",
          "type": "int",
          "description": "검색된 결과 개수 (default=20 max=100)"
        },
        {
          "name": "page",
          "type": "int",
          "description": "검색 결과 페이지 (default=1)"
        },
        {
          "name": "sort",
          "type": "string",
          "description": "정렬옵션 (기본 : lasc 별표서식명 오름차순), ldes(별표서식명 내림차순)"
        },
        {
          "name": "org",
          "type": "string",
          "description": "소관부처별 검색(소관부처코드 제공) 소관부처 2개이상 검색 가능(\",\"로 구분)"
        },
        {
          "name": "mulOrg",
          "type": "string",
          "description": "소관부처 2개이상 검색 조건 OR : OR검색 (default) AND : AND검색"
        },
        {
          "name": "knd",
          "type": "string",
          "description": "별표종류 1 : 별표 2 : 서식 3 : 별지 4 : 별도 5 : 부록"
        },
        {
          "name": "gana",
          "type": "string",
          "description": "사전식 검색(ga,na,da…,etc)"
        },
        {
          "name": "popYn",
          "type": "string",
          "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
        }
      ],
