    {
      "id": "1",
      "title": "행정규칙 목록 조회 API",
      "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=admrul",
      "target": "admrul",
      "api_type": "목록조회",
      "sub_category": "본문",
      "parameters": [
        {
          "name": "OC",
          "type": "string(필수)",
          "description": "사용자 이메일의 ID (g4c@korea.kr일경우 OC값=g4c)"
        },
        {
          "name": "target",
          "type": "string : admrul(필수)",
          "description": "서비스 대상"
        },
        {
          "name": "type",
          "type": "char(필수)",
          "description": "출력 형태 : HTML/XML/JSON"
        },
        {
          "name": "nw",
          "type": "int",
          "description": "(1: 현행, 2: 연혁, 기본값: 현행)"
        },
        {
          "name": "search",
          "type": "int",
          "description": "검색범위 (기본 : 1 행정규칙명) 2 : 본문검색"
        },
        {
          "name": "query",
          "type": "string",
          "description": "검색범위에서 검색을 원하는 질의 (정확한 검색을 위한 문자열 검색 query=\"자동차\")"
        },
        {
          "name": "display",
          "type": "int",
          "description": "검색된 결과 개수 (default=20 max=100)"
        },
        {
          "name": "page",
          "type": "int",
          "description": "검색 결과 페이지 (default=1)"
        },
        {
          "name": "org",
          "type": "string",
          "description": "소관부처별 검색(코드별도제공)"
        },
        {
          "name": "knd",
          "type": "string",
          "description": "행정규칙 종류별 검색 (1=훈령/2=예규/3=고시 /4=공고/5=지침/6=기타)"
        },
        {
          "name": "gana",
          "type": "string",
          "description": "사전식 검색 (ga,na,da…,etc)"
        },
        {
          "name": "sort",
          "type": "string",
          "description": "정렬옵션 (기본 : lasc 행정규칙명 오른차순) ldes 행정규칙명 내림차순 dasc : 발령일자 오름차순 ddes : 발령일자 내림차순 nasc : 발령번호 오름차순 ndes : 발령번호 내림차순 efasc : 시행일자 오름차순 efdes : 시행일자 내림차순"
        },
        {
          "name": "date",
          "type": "int",
          "description": "행정규칙 발령일자"
        },
        {
          "name": "prmlYd",
          "type": "string",
          "description": "발령일자 기간검색(20090101~20090130)"
        },
        {
          "name": "modYd",
          "type": "string",
          "description": "수정일자 기간검색(20090101~20090130)"
        },
        {
          "name": "nb",
          "type": "int",
          "description": "행정규칙 발령번호 ex)제2023-8호 검색을 원할시 nb=20238"
        },
        {
          "name": "popYn",
          "type": "string",
          "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
        }
      ],
      "sample_urls": [
